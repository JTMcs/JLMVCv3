<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arny.jlmvc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/trumpface"
        android:label="Animal Fun"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.arny.jlmvc.StartMenu">
            android:screenOrientation="portrait"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.arny.jlmvc.MainActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name="com.example.arny.jlmvc.EndGame">
            android:screenOrientation="portrait"
        </activity>
    </application>

</manifest>